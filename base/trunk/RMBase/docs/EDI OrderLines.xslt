<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">	<xsl:template match="/*">				<FMPXMLRESULT xmlns="http://www.filemaker.com/fmpxmlresult">			<ERRORCODE>0</ERRORCODE>			<PRODUCT BUILD="" NAME="" VERSION=""/>			<DATABASE DATEFORMAT="" LAYOUT="" NAME="mdm" RECORDS="{count(/*/order_line)}" TIMEFORMAT="MM/dd/yyyy"/>						<METADATA>				<FIELD EMPTYOK="NO" MAXREPEAT="1" NAME="id" TYPE="NUMBER" />				<FIELD EMPTYOK="NO" MAXREPEAT="1" NAME="kit_id" TYPE="TEXT" />				<FIELD EMPTYOK="YES" MAXREPEAT="1" NAME="kit_version" TYPE="TEXT" />				<FIELD EMPTYOK="NO" MAXREPEAT="1" NAME="quantity" TYPE="NUMBER" />				<FIELD EMPTYOK="YES" MAXREPEAT="1" NAME="wrapping_type" TYPE="NUMBER" />				<FIELD EMPTYOK="YES" MAXREPEAT="1" NAME="article_price" TYPE="NUMBER" />			</METADATA>						<RESULTSET>			<xsl:attribute name="FOUND"><xsl:value-of select="count(/*/order_line)"/></xsl:attribute>			<xsl:for-each select="/*/order_line">				<ROW MODID="0" RECORDID="0">					<xsl:for-each select=".">					<COL>						<DATA>							<xsl:value-of select="/id"/>						</DATA>					</COL>					<COL>						<DATA>							<xsl:value-of select="/kit_id"/>						</DATA>					</COL>					<COL>						<DATA>							<xsl:value-of select="/kit_version"/>						</DATA>					</COL>					<COL>						<DATA>							<xsl:value-of select="/quantity"/>						</DATA>					</COL>					<COL>						<DATA>							<xsl:value-of select="/wrapping_type"/>						</DATA>					</COL>					<COL>						<DATA>							<xsl:value-of select="/article_price"/>						</DATA>					</COL>					</xsl:for-each>				</ROW>			</xsl:for-each>			</RESULTSET>					</FMPXMLRESULT>			</xsl:template></xsl:stylesheet>