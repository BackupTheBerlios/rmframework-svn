<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<entry key="server.model.name">model.xml</entry>

	<entry key="server.rmi.port">10099</entry>


	<!-- memory, db, dual -->
	<entry key="server.operation.mode">memory</entry>
	<entry key="server.userManagement.initialize">true</entry>
	<entry key="server.db4o.defaultPath">db/database.dbo</entry>

	<!-- import section -->
	<entry key="server.import.folder">/tmp/import</entry>

	<entry key="server.communication">on</entry>
	
	<!-- jdbc connection / c3p0 -->
	<entry key="connection.jdbcDriver">com.ddtek.jdbc.sequelink.SequeLinkDriver</entry>
	<entry key="connection.jdbcUrl">jdbc:sequelink://localhost:2399</entry>
	<entry key="connection.user">Admin</entry>
	<entry key="connection.password">Avalon</entry>
</properties>