<?xml version="1.0" encoding="utf-8"?>
<data-map project-version="2.0">
	<property name="defaultPackage" value="net.form105.rm.server.model"/>
	<db-entity name="Customer">
		<db-attribute name="Creation Date" type="DATE" length="10"/>
		<db-attribute name="Creation Time" type="TIME" length="8"/>
		<db-attribute name="Creator" type="VARCHAR" length="1000000"/>
		<db-attribute name="Customer #" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="10"/>
		<db-attribute name="Max Ship Weight" type="DOUBLE" length="15"/>
		<db-attribute name="Modifier" type="VARCHAR" length="1000000"/>
		<db-attribute name="Resale ID" type="VARCHAR" length="1000000"/>
		<db-attribute name="Salesperson #" type="VARCHAR" length="1000000"/>
	</db-entity>
	<obj-entity name="Customer" className="net.form105.rm.server.model.Customer" dbEntityName="Customer">
		<obj-attribute name="creation_Date" type="java.util.Date" db-attribute-path="Creation Date"/>
		<obj-attribute name="creation_Time" type="java.util.Date" db-attribute-path="Creation Time"/>
		<obj-attribute name="creator" type="java.lang.String" db-attribute-path="Creator"/>
		<obj-attribute name="customer_pound" type="java.lang.String" db-attribute-path="Customer #"/>
		<obj-attribute name="max_Ship_Weight" type="java.lang.Double" db-attribute-path="Max Ship Weight"/>
		<obj-attribute name="modifier" type="java.lang.String" db-attribute-path="Modifier"/>
		<obj-attribute name="resale_ID" type="java.lang.String" db-attribute-path="Resale ID"/>
		<obj-attribute name="salesperson_pound" type="java.lang.String" db-attribute-path="Salesperson #"/>
	</obj-entity>
</data-map>
