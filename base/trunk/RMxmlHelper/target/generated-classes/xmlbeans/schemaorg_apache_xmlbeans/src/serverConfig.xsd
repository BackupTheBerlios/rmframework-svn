<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://xml.form105.net/schema/model"
	elementFormDefault="qualified" xmlns:rm="http://xml.form105.net/schema/model">

	<xsd:element name="config">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="server">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="id" maxOccurs="1" minOccurs="1" type="xsd:string"></xsd:element>
							<xsd:element name="alias" maxOccurs="1" minOccurs="1" type="xsd:string"></xsd:element>
							<xsd:element name="hostname" maxOccurs="1" type="xsd:string"></xsd:element>
							<xsd:element name="port" maxOccurs="1" minOccurs="1" type="xsd:integer"></xsd:element>
							<xsd:element name="type" maxOccurs="1" minOccurs="1"></xsd:element>
						</xsd:sequence>
						
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

</xsd:schema>