<?xml version="1.0" encoding="UTF-8"?>

<mapping>
	<configuration>
		<linestart>3</linestart>
		<mandate>default</mandate>
	</configuration>
	
	<object>
		<base>resource</base>
		<id>
			<prefix>part</prefix>
			<startNumber>generated</startNumber>
		</id>
		<type>sparePart</type>
		<name>Ersatzteil Förderanlage Lysbüchel</name>
		<department>coopConveyingSystem</department>
	</object>

	<field class="net.form105.rm.base.mapping.target.IntegerParameterTarget" fromStringMethod="valueByString">
		<id>position</id>
		<name>Position</name>
	</field>
	
	<field class="net.form105.rm.base.mapping.target.StringParameterTarget" fromStringMethod="valueByString">
		<id>articleNo</id>
		<name>Article Number</name>
	</field>
	
	<field class="net.form105.rm.base.mapping.target.StringParameterTarget" fromStringMethod="valueByString">
		<id>description</id>
		<name>Article Description</name>
	</field>
	
	<field class="net.form105.rm.base.mapping.target.StringParameterTarget" fromStringMethod="valueByString">
		<id>supplier</id>
		<name>Article Supplier</name>
	</field>
	
	<field class="net.form105.rm.base.mapping.target.IntegerParameterTarget" fromStringMethod="valueByString">
		<id>quantity</id>
		<type>integer</type>
		<name>Quantity</name>
	</field>
	
	<field class="net.form105.rm.base.mapping.target.IntegerParameterTarget" fromStringMethod="valueByString">
		<id>suggestQuantity</id>
		<type>integer</type>
		<name>Suggested Quantity</name>
	</field>
	
	<field class="net.form105.rm.base.mapping.target.IntegerParameterTarget" fromStringMethod="valueByString">
		<id>deliveryTime</id>
		<type>integer</type>
		<name>Delivery Time (Week)</name>
		<unit>week</unit>
	</field>
	
	<field class="net.form105.rm.base.mapping.target.FloatParameterTarget" fromStringMethod="valueByString">
		<id>pricePerPeace</id>
		<name>Price per Unit</name>
		<unit>currency</unit>
		<locale>de_ch</locale>
	</field>
	
</mapping>

